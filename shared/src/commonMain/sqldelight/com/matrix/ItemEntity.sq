insertItem:
INSERT INTO ItemEntity(
id,
patchVersion,
activeFlag,
childItemID,
deviceName,
glyph,
iconID,
itemTier,
price,
restrictedRoles,
rootItemID,
shortDesc,
startingItem,
type,
itemIconURL
)
VALUES(?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?);

selectAllItems:
SELECT *
FROM ItemEntity entity
JOIN ItemDescription AS desc ON entity.id = desc.itemEntityId
JOIN MenuItem AS menuItem ON desc.id = menuItem.itemDescriptionId;

selectItemById:
SELECT * FROM ItemEntity
WHERE id = ?;

